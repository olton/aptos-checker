div#buffer_block( data-role="panel" data-title-caption="BUFFER & CHANNEL" )
    div.w-100
        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 BUFFER DEQUEUED
                    div.text-leader2.text-bold.value#consensus_buffer_dequeued 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 BUFFER ENQUEUED
                    div.text-leader2.text-bold.value#consensus_buffer_enqueued 0

        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 CHANNEL DEQUEUED
                    div.text-leader2.text-bold.value#consensus_channel_dequeued 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 CHANNEL ENQUEUED
                    div.text-leader2.text-bold.value#consensus_channel_enqueued 0
            div.cell-12.border-top.bd-system
                div.text-bold.text-center
                    div.reduce-3 CHANNEL DROPPED
                    div.text-leader2.text-bold.value#consensus_channel_dropped 0


