div#round_block( data-role="panel" data-title-caption="ROUND" )
    div.w-100
        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 ROUND
                    div.text-leader2.text-bold.value#consensus_current_round 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 EPOCH
                    div.text-leader2.text-bold.value#consensus_epoch 0

        div.row.border-bottom.bd-system.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 LAST COMMITED ROUND
                    div.text-leader2.text-bold.value#consensus_last_commited_round 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 LAST COMMITED VERSION
                    div.text-leader2.text-bold.value#consensus_last_commited_version 0

        div.row.pb-2
            div.cell-6
                div.text-bold.text-center
                    div.reduce-3 EPOCH VALIDATORS
                    div.text-leader2.text-bold.value#consensus_current_epoch_validators 0
            div.cell-6.border-left.bd-system
                div.text-bold.text-center
                    div.reduce-3 ERROR COUNT
                    div.text-leader2.text-bold.value#consensus_error_count 0

